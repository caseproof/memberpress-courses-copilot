{
  "name": "memberpress-courses-copilot",
  "version": "1.0.0",
  "description": "AI-powered course creation copilot for MemberPress Courses",
  "private": true,
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass src/assets/scss:assets/css --style=compressed --source-map",
    "build:js": "webpack --mode=production",
    "dev": "npm run dev:css & npm run dev:js",
    "dev:css": "sass src/assets/scss:assets/css --watch --source-map",
    "dev:js": "webpack --mode=development --watch",
    "watch": "npm run dev",
    "lint:css": "stylelint 'src/assets/scss/**/*.scss'",
    "lint:js": "eslint 'src/assets/js/**/*.js'",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:fix": "npm run lint:css -- --fix && npm run lint:js -- --fix",
    "clean": "rimraf assets/css assets/js",
    "format": "prettier --write 'src/assets/**/*.{js,scss,css}'",
    "test:ai-chat": "node test-ai-chat-integration.js",
    "diagnostic": "node tests/diagnostic-check.js"
  },
  "keywords": [
    "wordpress",
    "memberpress",
    "courses",
    "ai",
    "copilot",
    "elearning"
  ],
  "author": "CaseProof",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.33.0",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^3.0.0",
    "puppeteer": "^22.15.0",
    "rimraf": "^5.0.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "stylelint": "^15.10.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
